    "workbench.colorCustomizations": {
        "activityBar.foreground": "#40826D",
        "activityBar.toggledBackground": "#40826D",
        "activityBar.activeBorder": "#40826D",
        "activityBarBadge.background": "#40826D",
        "button.background": "#40826D",
        "tab.activeBorderTop": "#40826D",
        "sideBar.dropBackground": "#40826D",
        "list.activeSelectionBackground": "#0b3025",
        "list.focusOutline": "#40826D",
        "terminal.selectionBackground": "#0b3025",
